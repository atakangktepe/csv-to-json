{
  "env": {
    "node": true
  },
  "rules": {
    "indent": [2, 2],
    "brace-style": [2, "1tbs"],
    "quotes": [2, "single"],
    "no-console": 1,
    "block-scoped-var": 2,
    "no-use-before-define": [2, "nofunc"],
    "no-underscore-dangle": 0,
    "no-underscore-dangle": 0,
    "space-before-function-paren": [2, {
      "anonymous": "always",
      "named": "never"
    }]
  }
}
